{
  "name": "school-timetable",
  "version": "1.0.0",
  "description": "School timetable generator",
  "main": "index.js",
  "scripts": {
    "lint:js": "eslint app/ webpack.*.js --cache",
    "lint:style": "stylelint app/assets/css/*.css",
    "prestart": "nsp check",
    "start": "nodemon --watch webpack.*.js --exec \"webpack-dev-server --config webpack.dev.js --progress --color\"",
    "build": "webpack --config webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "surge ./build",
    "localtunnel": "lt --local-host localhost --port 8080 --subdomain schooltimetable",
    "share:localtunnel": "npm-run-all --parallel start localtunnel"
  },
